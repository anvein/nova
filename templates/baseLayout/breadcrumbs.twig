<section
    class="page-title-section2 bg-img cover-background"
    data-overlay-dark="6"
    data-background="{{ breadcrumbsImageSrc | default('') }}"
    style="{{ breadcrumbsStyles | default('')  }}"
>
    <div class="container">

        <div class="row">
            <div class="col-md-12">
                <h1>{{ titleH1 | default('') }}</h1>
            </div>

            {% if breadcrumbs is not empty %}
                <div class="col-md-12">
                    <ul>
                        {% for item in breadcrumbs %}
                            <li><a href="{{ item.url | default('') }}">{{ item.text | default('') }}</a></li>
                        {% endfor %}
                    </ul>
                </div>
            {% endif %}
        </div>
    </div>
</section>